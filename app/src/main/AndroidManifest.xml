<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.gravitykerala.universityofcalicut" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".HomeDrawer"
            android:label="@string/app_name"
            android:theme="@style/MaterialDrawerTheme.Light.DarkToolbar.TranslucentStatus">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PareekshaBhavanActivity"
            android:label="@string/title_activity_pareeksha_bhavan" />
        <activity
            android:name=".DetailActivity"
            android:label="@string/title_activity_detail">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeDrawer" />
        </activity>
        <activity
            android:name=".Authorities"
            android:label="@string/title_activity_authorities"></activity>
        <activity
            android:name=".Syllabus"
            android:label="@string/title_activity_syllabus"></activity>
        <activity
            android:name=".Notification"
            android:label="@string/title_activity_notification"></activity>
        <activity
            android:name=".Result"
            android:label="@string/title_activity_result"></activity>
        <activity
            android:name=".TimeTable"
            android:label="@string/title_activity_time_table">
        </activity>
    </application>

</manifest>
